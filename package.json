    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "type": "module",
  "dependencies": {
    "@tanstack/svelte-query": "^4.22.2",
